{
  "name": "@kitledger/actions",
  "license": "Apache-2.0",
  "version": "0.0.14",
  "type": "module",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
	"./input": {
	  "types": "./dist/internal/input.d.ts",
	  "import": "./dist/internal/input.js"
	},
	"./project": {
		"types": "./dist/internal/project.d.ts",
		"import": "./dist/internal/project.js"
	},
	"./runtime": {
	  "types": "./dist/internal/runtime.d.ts",
	  "import": "./dist/internal/runtime.js"
	},
    ".": {
      "types": "./dist/index.d.ts",
	  "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@kitledger/query": "workspace:*",
	"@kitledger/sdk": "workspace:*"
  },
  "scripts": {
    "build": "tsc",
	"typecheck": "tsc --noEmit"
  }
}